FloatLayout:

    MDLabel:
        text: "Primero selecciona la mesa despues Load Food"
        halign: "center"
        pos_hint: {"center_x": .5, "center_y": .9}
        font_style: "H5"
        theme_text_color: "Custom"
        text_color: 0, 0, 0, 1
    MDIconButton:
        icon: "images/turtlebot_icon.png"
        icon_size: "45dp"
        pos_hint: {"center_x": .85, "center_y": .9}
    MDIconButton:
        icon: "images/ros_icon.png"
        icon_size: "60dp"
        pos_hint: {"center_x": .95, "center_y": .9}
    MDRoundFlatButton:
        id: load_food
        text: "Load Food"
        md_bg_color: "green"
        text_color: "white"
        pos_hint: {"center_x": .9, "center_y": .7} 
        on_press: app.load_food(*args)
    MDRoundFlatButton:
        id: unload_food
        text: "Unload Food"
        md_bg_color: "green"
        text_color: "white"
        pos_hint: {"center_x": .9, "center_y": .63} 
        on_press: app.unload_food(*args)
    MDRaisedButton:
        id: Home
        text: "Home"
        md_bg_color: "blue"
        pos_hint: {"center_x": .9, "center_y": .56} 
        on_press: app.home(*args)
    MDRaisedButton:
        id: T1
        text: "T1"
        md_bg_color: "blue"
        pos_hint: {"center_x": .9, "center_y": .49} 
        on_press: app.table1(*args)
    MDRaisedButton:
        id: T2
        text: "T2"
        md_bg_color: "blue"
        pos_hint: {"center_x": .9, "center_y": .42} 
        on_press: app.table2(*args)
    MDRaisedButton:
        id: T3
        text: "T3"
        md_bg_color: "blue"
        pos_hint: {"center_x": .9, "center_y": .35} 
        on_press: app.table3(*args)
    MDRaisedButton:
        id: T4
        text: "T4"
        md_bg_color: "blue"
        pos_hint: {"center_x": .9, "center_y": .28} 
        on_press: app.table4(*args)
    MDRaisedButton:
        id: T5
        text: "T5"
        md_bg_color: "blue"
        pos_hint: {"center_x": .9, "center_y": .21} 
        on_press: app.table5(*args)
    MDRaisedButton:
        id: T6
        text: "T6"
        md_bg_color: "blue"
        pos_hint: {"center_x": .9, "center_y": .14} 
        on_press: app.table6(*args)
    MDRaisedButton:
        id: Cancel
        text: "Cancel"
        md_bg_color: "red"
        pos_hint: {"center_x": .9, "center_y": .07} 
        on_press: app.cancel(*args)
    # CEFBrowser:
    #     id:cef_browser
    #     url: "google.com"
    #     size_hint: 0.5,0.5
    #     pos_hint: {'center_x':0.5,'center_y':0.44}
    MDIconButton:
        icon: "camera"
        icon_size: "64sp"
        pos_hint: {"center_x": .5, "center_y": .5}
        on_press: app.camara(*args)
    MDLabel:
        text: "Pulsa la camara para ver el robot en tiempo real"
        halign: "center"
        pos_hint: {"center_x": .5, "center_y": .3}
        font_style: "H5"
        theme_text_color: "Custom"
        text_color: 0, 0, 0, 1
    MDLabel:
        id: estado
        text: ""
        halign: "center"
        pos_hint: {"center_x": .5, "center_y": .2}
        font_style: "H5"
        theme_text_color: "Custom"
        text_color: 0, 0, 1, 1